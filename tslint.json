{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended",
		"tslint-config-standard",
		"tslint-react",
		"tslint-config-prettier"
	],
	"jsRules": {},
	"rules": {
		"ordered-imports": false,
		"object-literal-sort-keys": false,
		"member-ordering": false,
		"jsx-no-lambda": false,
		"jsx-boolean-value": false,
		"no-console": false,
		"triple-equals": false
	},
	"rulesDirectory": []
}
